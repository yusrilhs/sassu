    "dest": "./test/build",
    "encoding": "utf-8",
    "includePaths": [
        "./test/includes"
    ],
    "indentType": "space",
    "indentWidth": 4,
    "linefeed": "lf",
    "outputStyles": {
        "compressed": true,
        "expanded": true
    },
    "outputExtnames": {
        "compressed": ".min.css",
        "expanded": ".css"
